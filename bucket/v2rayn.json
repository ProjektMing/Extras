{
    "version": "6.60",
    "description": "A V2Ray client for Windows, support Xray & v2fly core",
    "homepage": "https://github.com/2dust/v2rayN",
    "license": "GPL-3.0-only",
    "depends": "xray",
    "suggest": {
        ".NET Desktop Runtime": "extras/windowsdesktop-runtime-lts",
        "v2fly-core": "v2ray"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/2dust/v2rayN/releases/download/6.60/v2rayN.zip",
            "hash": "fc1243faee5ebdf501d1416ae8567a993638c9ead92f94c4121feb6771f549d5"
        },
        "32bit": {
            "url": "https://github.com/2dust/v2rayN/releases/download/6.60/v2rayN-32.zip",
            "hash": "049cd293cd5bf2f7cfd5b3306428d9216323623de3301bc29f336b9ed0de42b2"
        },
        "arm64": {
            "url": "https://github.com/2dust/v2rayN/releases/download/6.60/v2rayN-arm64.zip",
            "hash": "31816af0a576c9540d1c73174ff193c12b5384d3f1fc16ac5aaee461778da397"
        }
    },
    "extract_dir": "v2rayN",
    "pre_install": [
        "if ($(appdir xray $global) -ne $null) {",
        "   New-Item -ItemType SymbolicLink -Path \"$dir\\bin\" -Name Xray -Target \"$(appdir xray $global)\\current\" -Force | Out-Null",
        "}",
        "if ($(appdir mihomo $global) -ne $null) {",
        "   New-Item -ItemType SymbolicLink -Path \"$dir\\bin\" -Name mihomo -Target \"$(appdir mihomo $global)\\current\" -Force | Out-Null",
        "}",
        "if ($(appdir sing-box $global) -ne $null) {",
        "   New-Item -ItemType SymbolicLink -Path \"$dir\\bin\" -Name sing_box -Target \"$(appdir sing-box $global)\\current\" -Force | Out-Null",
        "}",
        "if ($(appdir hysteria $global) -ne $null) {",
        "   New-Item -ItemType SymbolicLink -Path \"$dir\\bin\" -Name hysteria2 -Target \"$(appdir hysteria $global)\\current\" -Force | Out-Null",
        "}"
    ],
    "bin": "v2rayN.exe",
    "shortcuts": [
        [
            "v2rayN.exe",
            "v2rayN"
        ]
    ],
    "persist": "guiConfigs",
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/2dust/v2rayN/releases/download/$version/v2rayN.zip"
            },
            "32bit": {
                "url": "https://github.com/2dust/v2rayN/releases/download/$version/v2rayN-32.zip"
            },
            "arm64": {
                "url": "https://github.com/2dust/v2rayN/releases/download/$version/v2rayN-arm64.zip"
            }
        }
    }
}
